{
  "name": "current-weather",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-icons": "^3.2.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "thunk": "0.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:prettier/recommended",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard"
    ],
    "parser": "babel-eslint",
    "globals": {
      "window": true,
      "navigator": true
    },
    "rules": {
      "react/jsx-filename-extension": false,
      "react/prefer-stateless-function": false,
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "__REDUX_DEVTOOLS_EXTENSION__"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {},
        {
          "usePrettierrc": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "src/App.test.js",
    "src/index.js"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
